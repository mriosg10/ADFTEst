{
	"name": "Weather",
	"properties": {
		"activities": [
			{
				"name": "Weather",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "Staging",
								"type": "LinkedServiceReference"
							}
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['version']"
								},
								"sink": {
									"ordinal": 1
								}
							},
							{
								"source": {
									"path": "$['user']"
								},
								"sink": {
									"ordinal": 2
								}
							},
							{
								"source": {
									"path": "$['dateGenerated']"
								},
								"sink": {
									"ordinal": 3
								}
							},
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"ordinal": 4
								}
							},
							{
								"source": {
									"path": "$['data'][0]['parameter']"
								},
								"sink": {
									"ordinal": 5
								}
							},
							{
								"source": {
									"path": "$['data'][0]['coordinates'][0]['lat']"
								},
								"sink": {
									"ordinal": 6
								}
							},
							{
								"source": {
									"path": "$['data'][0]['coordinates'][0]['lon']"
								},
								"sink": {
									"ordinal": 7
								}
							},
							{
								"source": {
									"path": "$['data'][0]['coordinates'][0]['dates'][0]['date']"
								},
								"sink": {
									"ordinal": 8
								}
							},
							{
								"source": {
									"path": "$['data'][0]['coordinates'][0]['dates'][0]['value']"
								},
								"sink": {
									"ordinal": 9
								}
							}
						],
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "WeatherJSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlobMasterFAA",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}