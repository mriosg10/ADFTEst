										}
									},
									{
										"source": {
											"name": "CLASS 1",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "CLASS 1",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "0082",
