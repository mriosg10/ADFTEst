{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-test-marg"
		},
		"AmazonS31_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS31'"
		},
		"Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Staging'"
		},
		"AmazonS31_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIASZLKRDIJPXE7MB7W"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AmazonS31')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AmazonS3",
				"typeProperties": {
					"serviceUrl": "https://s3.amazonaws.com",
					"accessKeyId": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]",
					"secretAccessKey": {
						"type": "SecureString",
						"value": "[parameters('AmazonS31_secretAccessKey')]"
					},
					"authenticationType": "AccessKey"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Staging')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('Staging_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/S3Data')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AmazonS31",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AmazonS3Location",
						"bucketName": "adftest-marg",
						"fileName": "AcftRef.txt"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "MFR-NAME",
						"type": "String"
					},
					{
						"name": "TYPE-AIRCRAFT",
						"type": "String"
					},
					{
						"name": "TYPE-ENGINE",
						"type": "String"
					},
					{
						"name": "AC-CATEGORY",
						"type": "String"
					},
					{
						"name": "AMAC-TC-BUILT",
						"type": "String"
					},
					{
						"name": "NUMBER-ENGINES",
						"type": "String"
					},
					{
						"name": "NUMBER-SEATS",
						"type": "String"
					},
					{
						"name": "AC-WEIGHT",
						"type": "String"
					},
					{
						"name": "SPEED",
						"type": "String"
					},
					{
						"name": "MFR-MDL-CODE",
						"type": "String"
					},
					{
						"name": "KIT-CODE",
						"type": "String"
					},
					{
						"name": "EMPTY",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AmazonS31')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobMasterFAA')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "Staging",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "FAAaircraft.txt",
						"folderPath": "FAAaircraft",
						"container": "input"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "MFR-NAME",
						"type": "String"
					},
					{
						"name": "TYPE-AIRCRAFT",
						"type": "String"
					},
					{
						"name": "TYPE-ENGINE",
						"type": "String"
					},
					{
						"name": "AC-CATEGORY",
						"type": "String"
					},
					{
						"name": "AMAC-TC-BUILT",
						"type": "String"
					},
					{
						"name": "NUMBER-ENGINES",
						"type": "String"
					},
					{
						"name": "NUMBER-SEATS",
						"type": "String"
					},
					{
						"name": "AC-WEIGHT",
						"type": "String"
					},
					{
						"name": "SPEED",
						"type": "String"
					},
					{
						"name": "MFR-MDL-CODE",
						"type": "String"
					},
					{
						"name": "KIT-CODE",
						"type": "String"
					},
					{
						"name": "EMPTY",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Staging')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/IngestFromS3')]",
