										},
										"sink": {
											"name": "Prop_11",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "S3Data",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
